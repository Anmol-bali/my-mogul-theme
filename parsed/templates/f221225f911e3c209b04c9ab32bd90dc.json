{
  "components/faceted-search/facets/multi": "<ul\n  class=\"\n    {{#if selected}}is-open{{/if}}\n    {{#unless start_collapsed}}is-open{{/unless}}\n    {{#if items_count '<' total_items}}has-additional-filters{{/if}}\n  \"\n  id=\"filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n  data-count=\"{{items_count}}\"\n  data-facet=\"{{facet}}\"\n  data-facet-filter-wrapper\n  >\n\n  {{#each items}}\n    <li\n      class=\"{{#if @index '>=' 5}}hidden{{/if}} sidebar-filter-item filter-item\"\n      data-facet-item\n    >\n      <a href=\"{{url}}\"\n         class=\"\n           sidebar-filter-item-link\n           filter-item-link\n           {{#if selected}}sidebar-filter-item-link-selected{{/if}}\n          \"\n         data-faceted-search-facet>\n        {{{title}}}\n        {{#if ../show_product_counts}}\n          <span data-facet-item-count>({{count}})</span>\n        {{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n\n{{#if total_items \">\" 4}}\n  <a\n    class=\"\n      sidebar-filter-item-link-selected\n      sidebar-filter-item-link\n      filter-item-link\n    \"\n    href=\"#filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    data-show-more-facets\n  >\n    <span class=\"toggle-link-text toggle-link-text-off\">\n      {{lang 'filter.sort.show_more'}}\n    </span>\n    <span class=\"toggle-link-text toggle-link-text-on\">\n      {{lang 'filter.sort.show_less'}}\n    </span>\n  </a>\n{{/if}}\n"
}
